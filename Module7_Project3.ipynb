    "# transform variable to create common variables in the two data sets \n",
    "firm_data_filtered4_df[['Last_Name', 'First_Name']] = firm_data_filtered4_df['Employee_Name'].str.strip().str.split(',| ',expand=True)[[0,2]]\n",
